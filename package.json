{
  "name": "stylelint-itcss",
  "version": "0.1.0",
  "description": "A stylelint plugin to help with itcss architecture",
  "main": "dist/src/index.js",
  "scripts": {
    "compile": "tsc -watch -p ./",
    "lint": "node ./node_modules/.bin/tslint -c tslint.json --project tsconfig.json",
    "test": "tsc -b; ./node_modules/.bin/ava",
    "commit": "npx git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "itcss",
    "css"
  ],
  "author": "Benjamin JEGARD",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^8.0.0",
    "ava": "1.0.0-rc.2",
    "commitizen": "3.0.5",
    "conventional-changelog-cli": "2.0.11",
    "cz-conventional-changelog": "2.1.0",
    "stylelint": "9.9.0",
    "tslint": "5.11.0",
    "typescript": "3.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
